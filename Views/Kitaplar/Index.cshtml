                <form class="register-form" style="width:80%" action="../Kitaplar/Kitaplar" method="post">
                    <div class="form-title">
                        
                    </div>
                    <div class="form-group">
                        <table style="border-spacing:8px;">

                            <tr>
                                <td>
                                    ISBN:</td>

                                <td> <input class="form-control placeholder-no-fix" type="text" name="isbn" value=@ViewBag.isbnno /></td> 

                                <td>KITAP ADI:</td>
                                <td><input class="form-control placeholder-no-fix" type="text"   name="kitap_adi" value=@ViewBag.ad /></td>

                                <td>YAZAR:</td>
                                <td><input class="form-control placeholder-no-fix" type="text"   name="yazar" value=@ViewBag.yazar /></td>

                                <td>YAYIN EVI:</td>
                                <td><input class="form-control placeholder-no-fix" type="text"   name="yayin_evi" value=@ViewBag.yayinevi />
                                </td>

                            </tr>
                            <tr>

                                <td>BASIM YILI:</td>
                                <td><input class="form-control placeholder-no-fix" type="text"  name="basim_yili" value=@ViewBag.basim_yili />
                                  </td>



                                <td>KATEGORI:</td>
                                <td><input class="form-control placeholder-no-fix" type="text"  name="kategori" value=@ViewBag.kategori />
                                </td>

                                <td>ACIKLAMA:</td>
                                <td><input style="height:70px" class="form-control placeholder-no-fix" type="text"  name="aciklama" value=@ViewBag.aciklama />
                                </td>

                                <td>SAYFA SAYISI:</td>
                                <td><input class="form-control placeholder-no-fix" type="text"   name="sayfa_sayisi" value=@ViewBag.sayfasayisi />
                                </td>
                                <td>

                                    <input type="text" style="visibility:hidden" name="id" value=@ViewBag.id />
                                </td>
                            </tr>
                            <tr>
                                <td><div class="form-actions">
                     <button type="submit" id="register-submit-btn" class="btn red uppercase pull-left">Kaydet</button>



                        </div></td>
                                

                            </tr>

                          
                            
                        </table>

                    </div>



                    <div class="portlet box green" style="width:1000px;">
                        <div class="portlet-title" style="width:1000px;">
                            <div class="caption">
                                KITAPLAR
                            </div>

                        </div>
                        <div class="portlet-body flip-scroll" style="width:1000px;">
                            <table class="table table-bordered table-striped table-condensed flip-content" style="width:950px;">
                                <thead class="flip-content">
                                    <tr>
                                        <th width="10%">AD </th>
                                        <th> YAZAR </th>
                                        <th class="numeric" > SAYFA SAYISI </th>
                                        <th class="numeric"> YAYINEVİ </th>
                                        <th class="numeric"> BASIMYILI </th>
                                        <th class="numeric"> KATEGORİ </th>
                                        <th class="numeric"> ISBN </th>
                                        <th class="numeric"> AÇIKLAMA </th>
                                        <th class="numeric"> BASIM YILI </th>
                                        <th class="numeric"> KAYIT TARİHİ </th>
                                        <th class="numeric">İŞLEM  </th>
                                       
                                    </tr>
                                </thead>
                                <tbody>
                                     

                                    @foreach(var u in Model)
                                    {
                                        <tr>
                                        <td> @u.ad </td>
                                        <td> @u.yazar </td>
                                        <td class="numeric" >  @u.sayfasayisi </td>
                                        <td class="numeric"> @u.yayinevi </td>
                                        <td class="numeric"> @u.basimyili  </td>
                                        <td class="numeric"> @u.kategori </td>
                                        <td class="numeric"> @u.isbnno </td>
                                        <td class="numeric">@u.aciklama </td>
                                            <td class="numeric">@u.basim_yili </td>
                                            <td class="numeric">@u.kayit_tarih </td>
                                           

                                        <td class="numeric"> @Html.ActionLink("Delete", "Delete", new { id=u.id })
                                            @Html.ActionLink("Güncelle", "Index", new { id = u.id })</td>
                                    </tr>
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>


                 

</div>

