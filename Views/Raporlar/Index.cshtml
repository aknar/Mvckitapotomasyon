                <form class="register-form" action="../Raporlar/Index" method="post">

                    <table  >
                        <tr>
                            <td>T.C</td>
                            <td>
                                 <input class="form-control placeholder-no-fix" type="text" name="tc" />
                            </td>
                            <td>ISBN</td>
                            <td>
                                 <input class="form-control placeholder-no-fix" type="text"  name="isbn" />
                            </td>
                        </tr>
                     
                        <tr>
                             <td></td>
                            <td>  </td>
                            <td>
                                <div class="form-actions">
                                    <button type="submit" id="register-submit-btn" class="btn red uppercase pull-center">Ara</button>
                                </div>
                            </td>
                        </tr>
                    </table>

                   
                </form>
                <div class="portlet-body flip-scroll" style="width:1000px;">
                    <table class="table table-bordered table-striped table-condensed flip-content" style="width:950px;">
                        <thead class="flip-content">
                            <tr>
                                <th width="10%">AD </th>
                                <th> SOYAD </th>
                                <th class="numeric"> ISBN </th>
                                <th class="numeric"> KITAPAD </th>
                                <th class="numeric"> YAZAR </th>
                                <th class="numeric"> TARIH </th>
                                <th class="numeric"> TC </th>
                                    
                              
                            </tr>
                        </thead>
                       <tbody>
                           @foreach (var u in Model)
                           {
                               String tc = ViewBag.tc;
                               if (u.tc == tc)
                               {
                                   
                           

                               <tr>
                                   <td> @u.ad </td>
                                   <td> @u.soyad </td>
                                   <td class="numeric">  @u.isbnno </td>
                                   <td class="numeric"> @u.kitapad </td>
                                   <td class="numeric"> @u.yazar  </td>
                                   <td class="numeric"> @u.tarih </td>
                                   <td class="numeric"> @u.tc </td>



                               </tr>

                               }
                           }



                        </tbody> 
                    </table>
                </div>
                <div class="portlet-body flip-scroll" style="width:1000px;">
                    <table class="table table-bordered table-striped table-condensed flip-content" style="width:950px;">
                        <thead class="flip-content">
                         
                        </thead>
                        <tbody>
                            @foreach (var u in Model)
                            {
                                String isbn = ViewBag.isbn;
                                if (u.isbnno == isbn)
                                {



                                    <tr>
                                        <td> @u.ad </td>
                                        <td> @u.soyad </td>
                                        <td class="numeric">  @u.isbnno </td>
                                        <td class="numeric"> @u.kitapad </td>
                                        <td class="numeric"> @u.yazar  </td>
                                        <td class="numeric"> @u.tarih </td>
                                        <td class="numeric"> @u.tc </td>



                                    </tr>

                                }
                            }



                        </tbody>
                    </table>
                    <form action="../Raporlar/ExportData">
                        <input type="text" style="visibility:hidden" name="id" value=@ViewBag.tc />
                        <input type="text" style="visibility:hidden" name="id" value=@ViewBag.isbn />

                        <input type="submit" value="export" />
                    </form>
                </div>

